  },
  "dependencies": {
    "axios": "^0.27.2",
    "chalk": "^4.1.2",
    "console": "^0.7.2",
    "crypto-js": "^4.1.1",
    "dotenv": "^16.0.0",
    "inquirer": "^8.2.0",
    "prompt-sync": "^4.2.0"
  },
  "productName": "WORDLE",
  "devDependencies": {
    "electron": "^18.0.4",
    "electron-packager": "^15.5.1"
  }
}
